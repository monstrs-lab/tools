{
  "name": "@monstrs/config-jest",
  "version": "0.1.7",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@emotion/jest": "^11.10.8",
    "@monstrs/config-typescript": "workspace:0.1.5",
    "@monstrs/jest-resolver": "workspace:0.1.1",
    "jest-static-stubs": "^0.0.1",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.4"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
