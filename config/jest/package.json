{
  "name": "@monstrs/config-jest",
  "version": "0.3.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@emotion/jest": "^11.11.0",
    "@monstrs/config-typescript": "workspace:0.3.1",
    "@monstrs/jest-resolver": "workspace:0.3.1",
    "jest-static-stubs": "^0.0.1",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
