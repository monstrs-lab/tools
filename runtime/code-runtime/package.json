{
  "name": "@monstrs/tools-runtime",
  "version": "0.2.22",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./types": "./types/index.d.ts",
    ".": "./src/index.ts",
    "./eslint": "./src/eslint.ts",
    "./webpack": "./src/webpack.ts",
    "./jest": "./src/jest.ts",
    "./typescript": "./src/typescript.ts",
    "./svgr": "./src/svgr.ts"
  },
  "main": "src/index.ts",
  "types": "types/index.d.ts",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@jest/core": "^29.7.0",
    "@monstrs/config-eslint": "workspace:0.3.14",
    "@monstrs/config-jest": "workspace:0.3.15",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "eslint": "^8.57.0",
    "node-loader": "^2.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.26"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      "./types": "./types/index.d.ts",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./eslint": {
        "import": "./dist/eslint.js",
        "default": "./dist/eslint.js",
        "types": "./dist/eslint.d.ts"
      },
      "./webpack": {
        "import": "./dist/webpack.js",
        "default": "./dist/webpack.js",
        "types": "./dist/webpack.d.ts"
      },
      "./jest": {
        "import": "./dist/jest.js",
        "default": "./dist/jest.js",
        "types": "./dist/jest.d.ts"
      },
      "./typescript": {
        "import": "./dist/typescript.js",
        "default": "./dist/typescript.js",
        "types": "./dist/typescript.d.ts"
      },
      "./svgr": {
        "import": "./dist/svgr.js",
        "default": "./dist/svgr.js",
        "types": "./dist/svgr.d.ts"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
