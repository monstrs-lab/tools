{
  "name": "@monstrs/code-runtime",
  "version": "0.1.4",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./eslint": "./src/eslint.ts",
    "./webpack": "./src/webpack.ts",
    "./jest": "./src/jest.ts",
    "./typescript": "./src/typescript.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@jest/core": "^29.5.0",
    "@monstrs/config-eslint": "workspace:0.1.2",
    "@monstrs/config-jest": "workspace:0.1.3",
    "@monstrs/webpack-start-server-plugin": "workspace:0.3.3",
    "eslint": "^8.38.0",
    "node-loader": "^2.0.0",
    "string-replace-loader": "^3.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./eslint": {
        "import": "./dist/eslint.js",
        "default": "./dist/eslint.js",
        "types": "./dist/eslint.d.ts"
      },
      "./webpack": {
        "import": "./dist/webpack.js",
        "default": "./dist/webpack.js",
        "types": "./dist/webpack.d.ts"
      },
      "./jest": {
        "import": "./dist/jest.js",
        "default": "./dist/jest.js",
        "types": "./dist/jest.d.ts"
      },
      "./typescript": {
        "import": "./dist/typescript.js",
        "default": "./dist/typescript.js",
        "types": "./dist/typescript.d.ts"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
