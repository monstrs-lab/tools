{
  "name": "@monstrs/code-pack",
  "version": "1.0.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@actions/github": "^6.0.0",
    "@iarna/toml": "^2.2.5",
    "@yarnpkg/core": "^4.2.0",
    "@yarnpkg/fslib": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
