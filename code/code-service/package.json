{
  "name": "@monstrs/code-service",
  "version": "0.3.37",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist",
    "tsconfig.stub.json"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/code-configuration": "workspace:0.3.7",
    "@monstrs/config-typescript": "workspace:0.3.7",
    "@monstrs/tools-runtime": "workspace:0.2.24",
    "@monstrs/webpack-start-server-plugin": "workspace:0.5.9",
    "@yarnpkg/cli": "^4.3.1",
    "@yarnpkg/core": "^4.1.1",
    "typescript": "^5.5.3",
    "webpack-chain-5": "^8.0.2"
  },
  "devDependencies": {
    "@monstrs/logger": "^0.0.20",
    "@types/node": "^22.1.0",
    "@types/webpack": "^5.28.5",
    "@yarnpkg/fslib": "^3.1.0",
    "package-json-type": "^1.0.3"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
