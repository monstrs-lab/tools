{
  "name": "@monstrs/code-lint",
  "version": "0.0.121",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/code-runtime": "workspace:0.0.12",
    "globby": "^13.1.3",
    "ignore": "^5.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11"
  },
  "peerDependencies": {
    "react": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  }
}
