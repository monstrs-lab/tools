{
  "name": "@monstrs/yarn-plugin-cli-publish",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@yarnpkg/builder": "^4.0.0-rc.45",
    "@yarnpkg/cli": "^4.0.0-rc.45",
    "@yarnpkg/core": "^4.0.0-rc.45",
    "@yarnpkg/fslib": "^3.0.0-rc.45"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "^4",
    "@yarnpkg/core": "^4",
    "@yarnpkg/fslib": "^3"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
