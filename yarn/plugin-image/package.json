{
  "name": "@monstrs/yarn-plugin-image",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@monstrs/code-pack": "workspace:0.1.8",
    "@monstrs/yarn-pack-utils": "workspace:0.0.2",
    "@monstrs/yarn-run-utils": "workspace:0.0.2",
    "@yarnpkg/plugin-patch": "^4.0.0-rc.45",
    "clipanion": "^3.2.0"
  },
  "devDependencies": {
    "@yarnpkg/builder": "^4.0.0-rc.45",
    "@yarnpkg/cli": "^4.0.0-rc.45",
    "@yarnpkg/core": "^4.0.0-rc.45",
    "@yarnpkg/fslib": "^3.0.0-rc.45",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "^4",
    "@yarnpkg/core": "^4",
    "@yarnpkg/fslib": "^3"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
