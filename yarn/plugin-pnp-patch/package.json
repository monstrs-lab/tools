{
  "name": "@monstrs/yarn-plugin-pnp-patch",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:loader && yarn run build:source",
    "build:loader": "rollup -c",
    "build:source": "builder build plugin && mv bundles/@yarnpkg/plugin-pnp-patch.js bundles/@yarnpkg/plugin-pnp-patch.cjs",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist",
    "yarn:download": "yarn node ./scripts/download-yarn-pnp-packages.script.mjs"
  },
  "dependencies": {
    "@yarnpkg/builder": "^4.0.0-rc.45",
    "@yarnpkg/core": "^4.0.0-rc.45",
    "@yarnpkg/fslib": "^3.0.0-rc.45",
    "@yarnpkg/plugin-pnp": "^4.0.0-rc.45",
    "@yarnpkg/pnp": "file:./cache/yarnpkg-pnp.tgz"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "esbuild": "^0.17.18",
    "rollup": "^3.21.5",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
