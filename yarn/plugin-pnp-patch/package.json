{
  "name": "@monstrs/yarn-plugin-pnp-patch",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:loader && yarn run build:source",
    "build:loader": "rollup -c",
    "build:source": "builder build plugin && mv bundles/@yarnpkg/plugin-pnp-patch.js bundles/@yarnpkg/plugin-pnp-patch.cjs",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist",
    "yarn:download": "yarn node ./scripts/download-yarn-pnp-packages.script.mjs"
  },
  "dependencies": {
    "@yarnpkg/builder": "^3.2.7",
    "@yarnpkg/core": "^3.5.0",
    "@yarnpkg/fslib": "^2.10.2",
    "@yarnpkg/plugin-pnp": "^3.2.8",
    "@yarnpkg/pnp": "file:./cache/yarnpkg-pnp.tgz"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "esbuild": "^0.17.15",
    "rollup": "3.20.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
