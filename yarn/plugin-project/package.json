{
  "name": "@monstrs/yarn-plugin-project",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/tools-runtime": "workspace:0.0.3",
    "clipanion": "^3.2.0",
    "semver": "^7.5.0"
  },
  "devDependencies": {
    "@yarnpkg/builder": "^4.0.0-rc.43",
    "@yarnpkg/cli": "^4.0.0-rc.43",
    "@yarnpkg/core": "^4.0.0-rc.43",
    "@yarnpkg/fslib": "^3.0.0-rc.43"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
