  "dependencies": {
    "@monstrs/code-format-worker": "workspace:^",
    "@monstrs/code-icons-worker": "workspace:^",
    "@monstrs/code-lint-worker": "workspace:^",
    "clipanion": "^3.2.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@monstrs/yarn-run-utils": "workspace:0.0.1",
    "@monstrs/yarn-test-utils": "workspace:0.0.1",
    "@yarnpkg/builder": "^4.0.0-rc.43",
    "@yarnpkg/cli": "^4.0.0-rc.43",
    "@yarnpkg/core": "^4.0.0-rc.43"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "^4",
    "@yarnpkg/core": "^4"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
