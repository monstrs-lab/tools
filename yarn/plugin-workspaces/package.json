  },
  "devDependencies": {
    "@yarnpkg/builder": "^3.2.7",
    "@yarnpkg/cli": "^3.5.0",
    "@yarnpkg/core": "^3.5.0",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "^3.2.0-rc.7",
    "@yarnpkg/core": "^3.2.0-rc.7"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
