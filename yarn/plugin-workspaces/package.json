    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/yarn-plugin-files": "workspace:0.0.1",
    "@monstrs/yarn-workspace-utils": "workspace:0.0.1",
