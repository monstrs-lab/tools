{
  "name": "@monstrs/yarn-cli",
  "version": "0.2.10",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:clean && yarn run build:bundle && yarn run build:fix && yarn run build:dist",
    "build:bundle": "builder build bundle && mv bundles/yarn.js bundles/yarn.cjs",
    "build:clean": "rm -rf dist",
    "build:dist": "mv bundles dist",
    "build:fix": "yarn node patches/cli.patch.mjs",
    "fix": "yarn run build:fix",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/yarn-cli-tools": "workspace:0.2.10",
    "@monstrs/yarn-plugin-check": "workspace:0.2.1",
    "@monstrs/yarn-plugin-checks": "workspace:0.2.1",
    "@monstrs/yarn-plugin-cli-publish": "workspace:0.2.0",
    "@monstrs/yarn-plugin-commit": "workspace:0.2.1",
    "@monstrs/yarn-plugin-essentials": "workspace:0.2.1",
    "@monstrs/yarn-plugin-files": "workspace:0.2.1",
    "@monstrs/yarn-plugin-format": "workspace:0.2.1",
    "@monstrs/yarn-plugin-image": "workspace:0.2.1",
    "@monstrs/yarn-plugin-library": "workspace:0.2.1",
    "@monstrs/yarn-plugin-lint": "workspace:0.2.1",
    "@monstrs/yarn-plugin-renderer": "workspace:0.2.1",
    "@monstrs/yarn-plugin-service": "workspace:0.2.1",
    "@monstrs/yarn-plugin-test": "workspace:0.2.1",
    "@monstrs/yarn-plugin-tools": "workspace:0.2.1",
    "@monstrs/yarn-plugin-types": "workspace:0.2.1",
    "@monstrs/yarn-plugin-ui": "workspace:0.2.1",
    "@monstrs/yarn-plugin-workspaces": "workspace:0.2.1",
    "@yarnpkg/builder": "^4.0.0-rc.45",
    "@yarnpkg/cli": "^4.0.0-rc.45",
    "@yarnpkg/core": "^4.0.0-rc.45",
    "@yarnpkg/fslib": "^3.0.0-rc.45",
    "@yarnpkg/libzip": "^3.0.0-rc.45",
    "@yarnpkg/parsers": "^3.0.0-rc.45",
    "@yarnpkg/plugin-compat": "^4.0.0-rc.45",
    "@yarnpkg/plugin-dlx": "^4.0.0-rc.45",
    "@yarnpkg/plugin-essentials": "^4.0.0-rc.45",
    "@yarnpkg/plugin-file": "^3.0.0-rc.45",
    "@yarnpkg/plugin-git": "^3.0.0-rc.45",
    "@yarnpkg/plugin-github": "^3.0.0-rc.45",
    "@yarnpkg/plugin-http": "^3.0.0-rc.45",
    "@yarnpkg/plugin-init": "^4.0.0-rc.45",
    "@yarnpkg/plugin-interactive-tools": "^4.0.0-rc.45",
    "@yarnpkg/plugin-link": "^3.0.0-rc.45",
    "@yarnpkg/plugin-nm": "^4.0.0-rc.45",
    "@yarnpkg/plugin-npm": "^3.0.0-rc.45",
    "@yarnpkg/plugin-npm-cli": "^4.0.0-rc.45",
    "@yarnpkg/plugin-pack": "^4.0.0-rc.45",
    "@yarnpkg/plugin-patch": "^4.0.0-rc.45",
    "@yarnpkg/plugin-pnp": "^4.0.0-rc.45",
