    "@yarnpkg/plugin-workspace-tools": "^4.0.0-rc.45",
    "@yarnpkg/shell": "^4.0.0-rc.45",
    "clipanion": "^3.2.0",
    "semver": "^7.5.1",
    "typanion": "^3.12.1"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "@yarnpkg/builder": {
    "bundles": {
      "standard": [
        "@monstrs/yarn-plugin-check",
