{
  "name": "@monstrs/yarn-pack-utils",
  "version": "0.2.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@yarnpkg/cli": "^4.0.0-rc.45",
    "@yarnpkg/core": "^4.0.0-rc.45",
    "@yarnpkg/fslib": "^3.0.0-rc.45",
    "@yarnpkg/plugin-pack": "^4.0.0-rc.45",
    "@yarnpkg/plugin-patch": "^4.0.0-rc.45",
    "semver": "^7.5.1",
    "tar-stream": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
